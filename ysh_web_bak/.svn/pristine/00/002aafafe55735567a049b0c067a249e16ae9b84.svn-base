	margin: 1rem auto;
}

.code-box p {
	color: #ccc;
	font-size: .4rem;
	text-align: center;
}

.code-box .tit {
	color: black;
	margin-top: 2rem;
	font-size: .6rem;
}

#arrow_up {
	position: fixed;
	right: 1rem;
	top: 0;
	z-index: 9999;
	color: white;
	font-size: .7rem;
	width: 3rem;
	height: 2rem;
	line-height: 2rem;
	text-align: center;
	background: rgba(0, 0, 0, .3);
	border-bottom-left-radius: 25%;
	border-bottom-right-radius: 25%;
}

#recoldOver {
	display: block;
	text-align: center;
	margin-top: 4rem;
}

#maskTime {
	font-size: 56px;
	color: white;
	position: absolute;
	width: 100%;
	text-align: center;
	top: 35%;
	display: none;
	background: transparent;
	left: -.2rem;
}

dt i time {
	color: #fc9327;
	padding-left: .3rem;
}

.loading {
	position: absolute;
	left: 25%;
	top: 50%;
	z-index: 9999999999;
	background: rgba(0, 0, 0);
	background: rgba(0, 0, 0, .89);
	width: 56%;
	height: 5rem;
	width: 50%;
	border-radius: 5px;
	box-shadow: 0 0 6px rgba(0, 0, 0, .4);
	margin-top: -2.5rem;
	text-align: center;
	color: white;
	text-shadow: 0px 0px 1px rgba(47, 47, 47, 0.75);
	display: none;
}

.loading img {
	width: 2rem;
	height: 2rem;
	display: block;
	margin: .8rem auto;
}

#noLogin {
	position: absolute;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	z-index: 9999999999;
	background: white;
	text-align: center;
	display: none;
}

#noLogin h1 {
	font-size: .8rem;
	margin-top: 3rem;
}

#noLogin a {
	width: 5rem;
	height: 1.5rem;
	background: #41C3A8;
	display: block;
	margin: 2rem auto;
	line-height: 1.5rem;
	text-align: center;
	color: white;
	font-size: .6rem;
	letter-spacing: 2px;
	border-radius: 20px;
}

.audioMask {
	width: 100%;
	height: 10rem;
	background: black;
	position: absolute;
	z-index: 999999999;
	left: 0;
	top: 0;
	opacity: .3;
	background: url(http://weixin.keepyoga.com/images/player.png) no-repeat;
	background-size: 20%;
	background-position-x: 50%;
	background-position-y: 50%;
}