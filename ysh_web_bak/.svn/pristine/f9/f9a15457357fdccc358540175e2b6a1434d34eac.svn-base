html,
body {
	width: 100%;
	height: 100%;
}

.wrapper {
	width: 100%;
	height: 100%;
	background: #f4f4f4;
	overflow: hidden;
	-webkit-overflow-scrolling: touch;
}

header {
	width: 17rem;
	height: 2.65rem;
	background: white;
	margin: 0 auto;
	padding-bottom: 1px;
}

header h1 {
	width: 15.85rem;
	height: inherit;
	border-bottom: 1px solid #f4f4f4;
	line-height: 2.65rem;
	font-size: .7rem;
	color: #9c9c9c;
	margin: .375rem auto 0;
	text-align: center;
}

.registerBox {
	display: none;
}

.userInfo {
	width: 17rem;
	margin: 0 auto;
}

.userInfoBox {
	width: 17rem;
	margin: 0 auto;
	background: white;
}

.userInfo ul {
	width: 15.85rem;
	margin: 0 auto;
}

.userInfo ul li {
	width: 100%;
	height: 2rem;
	line-height: 2rem;
	display: -webkit-box;
	padding: .35rem 0;
	border-bottom: 1px solid #f4f4f4;
}

.userInfo ul label {
	display: block;
	width: 4rem;
	text-align: center;
	letter-spacing: .1rem;
	font-size: .7rem;
}

.userInfo ul input {
	-webkit-box-flex: 1;
	background: #f8f8f8;
	display: block;
	height: 2rem;
	line-height: 2rem;
	font-size: .65rem;
	color: #3f3f3f;
	text-indent: 1rem;
	border-radius: 3px;
}

form button {
	width: 100%;
	height: 2rem;
	border-radius: 5px;
	background: #118eff;
	display: block;
	outline: none;
	border: none;
	font-size: .8rem;
	color: white;
	margin-top: .55rem;
}

.sucessBox {
	width: 100%;
	height: 11rem;
	margin-top: .75rem;
	background: white;
	padding-top: 1.1rem;
	display: none;
}

.sucessBox h1 {
	font-size: .8rem;
	text-align: center;
	color: #fc9426;
}

.sucessBox h2 {
	font-size: .55rem;
	color: #c6c6c6;
	text-align: center;
	margin: .675rem 0 .3rem 0;
}

.sucessBox h3 {
	font-size: .45rem;
	color: #c6c6c6;
	text-align: center;
	margin-bottom: 1.1rem;
}

.sucessBox button {
	display: block;
	width: 9.6rem;
	height: 1.7rem;
	outline: none;
	border: none;
	border-radius: 5px;
	background: #1d92fb;
	color: white;
	font-size: .7rem;
	margin: 0 auto;
}

.sucessBox img {
	width: 2.7rem;
	height: 2.7rem;
	display: block;
	margin: 0 auto .9rem;
}

.masking {
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, .8);
	position: absolute;
	left: 0;
	top: 0;
	display: none;
}

.share {
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 1000;
	display: none;
}

.share img {
	width: 60%;
	height: auto;
	margin-left: 35%;
	margin-top: 1rem;
}

.modelTips {
	display: none;
	position: absolute;
	left: 50%;
	top: 50%;
	width: 16rem;
	height: 4rem;
	border-radius: 5px;
	z-index: 99999;
	background: rgba(0, 0, 0, .6);
	margin-left: -8rem;
	margin-top: -2rem;
}

.modelTips p {
	color: white;
	font-size: .8rem;
	line-height: 4rem;
	text-align: center;
}

a {
	font-size: 14px;
	color: red;
}

.getyzm {
	position: absolute;
	color: red;
	font-size: 16px;
	right: 2rem;
}

.check {
	display: inline-block;
	background: url("http://wimg.keepyoga.com/images/ysh-check.png") no-repeat;
	background-size: cover;
	width: 15px;
	height: 15px;
	margin-left: 30%;
	position: relative;
	top: 2px;
}

.check.high {
	background-position: 0 -17px;
}